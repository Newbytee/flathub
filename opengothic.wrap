#!/bin/bash
# this should be replaced by something better eventually, but
# for now I add it here to let GUI invocation do something.

POSITIONAL=()
while [[ $# -gt 0 ]]
do
key="$1"

case $key in
	-i|--interactive)
	echo 'hi'
	shift
	;;
	*)
	POSITIONAL+=("$1")
	shift
	;;
esac
done

set -- "${POSITIONAL[@]}"

echo $@
